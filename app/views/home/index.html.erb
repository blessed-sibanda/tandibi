<h1 class="text-4xl text-blue-700 font-semibold">Home#index</h1>
<p>Find me in app/views/home/index.html.erb</p>

<% if user_signed_in? %>
  <%= button_to 'Sign out', destroy_user_session_path, method: :delete, class: 'px-4 py-2 bg-gray-400 text-white' %>
<% else %>
